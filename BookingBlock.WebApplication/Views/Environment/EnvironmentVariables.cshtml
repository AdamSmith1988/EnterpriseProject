@using System.Collections
@model System.Collections.IDictionary
@{
    ViewBag.Title = "App Settings";
}

<h2>App Settings</h2>

<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Variable</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach (DictionaryEntry entry in Model)
            {
                <tr>
                    <td>@entry.Key</td>
                    <td>@entry.Value</td>
                </tr>
            }

        </tbody>

    </table>
</div> 